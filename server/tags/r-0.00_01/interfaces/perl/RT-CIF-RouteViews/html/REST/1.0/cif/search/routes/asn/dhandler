<%ARGS>
</%ARGS>
<%INIT>
use CIF::RouteViews;
use RT::Interface::REST;

my $output = "";
my $status = "200 Ok";

my $arg = $m->dhandler_arg();
my @recs = CIF::RouteViews->search(asn => $arg);

$output = $m->comp('../text', recs => \@recs);

$m->out("RT/". $RT::VERSION . " " . $status ."\n\n");
$m->out($output);
</%INIT>
