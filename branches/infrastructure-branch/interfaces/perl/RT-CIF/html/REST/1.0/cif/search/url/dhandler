<%ARGS>
$format     => 'text'
$qlimit     => 1500,
</%ARGS>
<%INIT>
use RT::Interface::REST;

my $arg = $m->dhandler_arg();

my @recs;

for($arg){
    if(/^[a-zA-Z0-9]{32}$/){
        $m->comp('url_md5/dhandler', format => $format, $qlimit => $qlimit);
        last;
    }
    if(/^[a-zA-Z0-9]{40}$/){
        $m->comp('url_sha1/dhandler', format => $format, $qlimit => $qlimit);
        last;
    }
}

</%INIT>
